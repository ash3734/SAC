<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="test.SACAPP" >

    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="test.SACAPP.LogIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="test.SACAPP.Main" android:label="@layout/activity_main"></activity>
        <activity android:name="test.SACAPP.MyClassProf" android:label="@layout/activity_myclass_prof"></activity>
        <activity android:name="test.SACAPP.MyClassStud" android:label="@layout/activity_myclass_stud"></activity>
        <activity android:name="test.SACAPP.WeeksProf" android:label="@layout/activity_weeks_prof"></activity>
        <activity android:name="test.SACAPP.WeeksStud" android:label="@layout/activity_weeks_stud"></activity>
    </application>

</manifest>
